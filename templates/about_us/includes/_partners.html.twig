<section class="links" id="nos-partenaires">
    <div class="bgPartner pt-4 pt-md-5 pb-5">
        <h2 class="text-center">Nos partenaires</h2>
        <h3 class="text-center m-4">Ils nous inspirent ou nous soutiennent</h3>
        <div class="container pt-4">
            <div class="row no-gutters">
                {% for i in 1..12 %}
                    <div class="logo-partner text-center col-lg-3 col-md-4 col-xs-6">
                        <img class="img-fluid img-partner"
                             src="{{ asset('build/images/partenaires/partenaire-'~i~'.jpg') }}"
                             alt="{{ asset('partenaire-'~i~'.jpg') }}">
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
</section>
