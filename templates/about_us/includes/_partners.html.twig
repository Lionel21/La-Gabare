<section class="bgPartner p-5">
    <h2 class="text-center mt-2">Nos partenaires</h2>
    <h3 class="text-center m-5">Ils nous inspirent ou nous soutiennent</h3>
    <div class="container">
        <div class="row no-gutters m-2">
            {% for partner in partners %}
            <div class="logo-partner text-center col-lg-3 col-md-4 col-xs-6">
                <img class="img-fluid img-partner" src="{{ partner.image }}"
                     alt={{ partner.name }}>
            </div>
            {% endfor %}
        </div>
    </div>
</section>
